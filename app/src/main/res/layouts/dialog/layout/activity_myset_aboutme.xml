<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fragment_background"
    tools:context=".person.activity.MySet_AboutMe_Activity"
    tools:ignore="MissingConstraints">

    <com.xunda.lib.common.common.titlebar.CustomTitleBar
        android:id="@+id/title_bar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height"
        app:titleBarLeftImg="@mipmap/icon_back"
        app:titleBarShowBottomLine="false"
        app:titleBarTitle="关于我们" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/dp_68"
        android:layout_height="@dimen/dp_68"
        android:layout_marginTop="@dimen/dp_68"
        android:background="@mipmap/logo_share"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/title_bar" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        android:text="发现扫地僧"
        android:textColor="@color/text_color_444444"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/tv_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        android:text="当前版本1.0.0"
        android:textColor="@color/text_color_444444"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_42"
        android:layout_marginTop="@dimen/dp_75"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/tv_version"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_15"
            android:text="客服热线"
            android:textColor="@color/text_color_444444"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="107dp"
            android:layout_height="17dp"
            android:layout_marginEnd="@dimen/dp_15"
            android:text="（021）5071 9670"
            android:textColor="#ff1f8fe5"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <com.xunda.lib.common.view.MyArrowItemView
        android:id="@+id/update_MyView"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_43"
        android:background="@color/white"
        app:arrowItemAvatarHeight="@dimen/dp_12"
        app:arrowItemContent=""
        app:arrowItemShowAvatar="false"
        app:arrowItemTitle="版本更新"
        app:arrowItemCountTv="false"
        app:arrowItemTitleColor="@color/text_color_444444"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout6" />



    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp_25"
        android:text="              Copyright@2021\n
北京清博大数据科技有限公司版权所有"
        android:textColor="@color/text_color_a1a1a1"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/service_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp_15"
        android:text="用户协议"
        android:textColor="@color/text_color_1F8FE5"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@id/privacy_Tv"
        app:layout_constraintBottom_toTopOf="@+id/textView16"
        app:layout_constraintStart_toStartOf="@+id/textView16" />


    <TextView
        android:id="@+id/privacy_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp_15"
        android:text="隐私政策"
        android:textColor="@color/text_color_1F8FE5"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/textView16"
        app:layout_constraintEnd_toEndOf="@+id/textView16"
        app:layout_constraintStart_toEndOf="@+id/service_Tv" />

</androidx.constraintlayout.widget.ConstraintLayout>