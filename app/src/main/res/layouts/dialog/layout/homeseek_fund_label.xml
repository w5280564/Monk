<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_43"
    tools:ignore="MissingConstraints">

    <TextView
        android:id="@+id/fundName_Tv"
        android:layout_width="@dimen/dp_130"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:text="科大讯飞科大讯飞科大讯飞科大讯飞科大讯飞科大讯飞"
        android:textColor="@color/text_color_444444"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="@dimen/dp_15"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fundCode_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SH600000"
        android:textColor="@color/text_color_444444"
        android:layout_marginStart="@dimen/dp_15"
        android:textSize="12sp"
        app:layout_constraintTop_toTopOf="@id/fundName_Tv"
        app:layout_constraintBottom_toBottomOf="@id/fundName_Tv"
        app:layout_constraintStart_toEndOf="@+id/fundName_Tv" />


    <View
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:layout_marginTop="@dimen/dp_20"
        android:background="@color/text_color_CCCCCC"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="@dimen/dp_15"
        app:layout_constraintStart_toStartOf="@+id/fundName_Tv"
        app:layout_constraintTop_toBottomOf="@id/fundCode_Tv" />

</androidx.constraintlayout.widget.ConstraintLayout>