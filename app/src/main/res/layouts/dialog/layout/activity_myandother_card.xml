<?xml version="1.0" encoding="utf-8"?>
<com.qingbo.monk.base.CustomCoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/headLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <!--    app:layout_behavior=".base.AppBarLayoutOverScrollViewBehavior"-->
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar_Layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.google.android.material.appbar.CollapsingToolbarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:contentScrim="@color/white"
            app:layout_scrollFlags="scroll">

            <ImageView
                android:id="@+id/iv_img"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@mipmap/card_bg"
                android:scaleType="centerCrop"
                app:layout_collapseMode="parallax" />

            <Button
                android:id="@+id/back_Btn"
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_18"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_33"
                android:background="@mipmap/icon_back"
                android:backgroundTint="@color/white"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <androidx.core.widget.NestedScrollView
                android:id="@+id/top_Src"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:tag="middle">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/top_Con"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_135"
                    android:background="@drawable/base_gradual_top"
                    app:layout_constraintTop_toBottomOf="@id/back_Btn"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:layout_width="@dimen/dp_67"
                        android:layout_height="@dimen/dp_28"
                        android:background="@drawable/shape_bag_join_group"
                        android:text="编辑"
                        android:gravity="center_vertical"
                        android:drawableStart="@mipmap/bianji_pen"
                        android:paddingStart="@dimen/dp_10"
                        android:drawablePadding="@dimen/dp_10"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginTop="@dimen/dp_30"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginEnd="@dimen/dp_15"
                        android:textColor="@color/text_color_a1a1a1"
                        android:textSize="12sp" />


                    <ImageView
                        android:id="@+id/head_Img"
                        android:layout_width="@dimen/dp_56"
                        android:layout_height="@dimen/dp_56"
                        android:layout_marginStart="@dimen/dp_15"
                        android:layout_marginTop="@dimen/dp_15"
                        android:background="@mipmap/icon_logo"
                        android:scaleType="centerCrop"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <TextView
                        android:id="@+id/tv_follow_number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_15"
                        android:text="55"
                        android:textColor="@color/text_color_444444"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toTopOf="@id/textview10"
                        app:layout_constraintStart_toEndOf="@id/head_Img"
                        app:layout_constraintTop_toTopOf="@+id/head_Img" />

                    <TextView
                        android:id="@+id/textview10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="关注"
                        android:textColor="@color/text_color_a1a1a1"
                        android:textSize="@dimen/sp_12"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/head_Img"
                        app:layout_constraintEnd_toEndOf="@+id/tv_follow_number"
                        app:layout_constraintStart_toStartOf="@+id/tv_follow_number"
                        app:layout_constraintTop_toBottomOf="@+id/tv_follow_number" />


                    <TextView
                        android:id="@+id/tv_fans_number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_30"
                        android:text="55"
                        android:textColor="@color/text_color_444444"
                        android:textSize="@dimen/sp_15"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@id/tv_follow_number"
                        app:layout_constraintStart_toEndOf="@id/tv_follow_number"
                        app:layout_constraintTop_toTopOf="@id/tv_follow_number" />

                    <TextView
                        android:id="@+id/textview11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="粉丝"
                        android:textColor="@color/text_color_a1a1a1"
                        android:textSize="@dimen/sp_12"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/head_Img"
                        app:layout_constraintEnd_toEndOf="@+id/tv_fans_number"
                        app:layout_constraintStart_toStartOf="@+id/tv_fans_number"
                        app:layout_constraintTop_toBottomOf="@+id/tv_fans_number" />


                    <TextView
                        android:id="@+id/tv_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_15"
                        android:maxLength="6"
                        android:text="一位神秘的人"
                        android:textColor="@color/text_color_444444"
                        android:textSize="18sp"
                        app:layout_constraintStart_toStartOf="@+id/head_Img"
                        app:layout_constraintTop_toBottomOf="@+id/head_Img" />


                    <TextView
                        android:id="@+id/brief_Tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_15"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginEnd="@dimen/dp_25"
                        android:ellipsize="end"
                        android:maxLines="2"
                        android:textColor="@color/text_color_a1a1a1"
                        android:textSize="12sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_name" />


                    <LinearLayout
                        android:id="@+id/label_Lin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_15"
                        android:layout_marginTop="@dimen/dp_20"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/brief_Tv" />

                    <TextView
                        android:id="@+id/address_Tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_15"
                        android:layout_marginTop="@dimen/dp_15"
                        android:drawableStart="@mipmap/weizhi"
                        android:drawablePadding="@dimen/dp_10"
                        android:text="城市："
                        android:textColor="@color/text_color_a1a1a1"
                        android:textSize="12sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/label_Lin" />

                    <TextView
                        android:id="@+id/industry_Tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_15"
                        android:layout_marginTop="@dimen/dp_15"
                        android:drawableStart="@mipmap/hangye"
                        android:drawablePadding="@dimen/dp_10"
                        android:text="行业："
                        android:textColor="@color/text_color_a1a1a1"
                        android:textSize="12sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/address_Tv" />

                    <TextView
                        android:id="@+id/job_Tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_15"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:drawableStart="@mipmap/jingyan"
                        android:drawablePadding="@dimen/dp_10"
                        android:text="工作经验："
                        android:textColor="@color/text_color_a1a1a1"
                        android:textSize="12sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/industry_Tv" />

                    <View
                        android:id="@+id/lineView"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:background="@color/dl_background"
                        app:layout_constraintTop_toBottomOf="@id/job_Tv" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/home_Con"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_50"
                        android:background="@color/white"
                        app:layout_constraintTop_toBottomOf="@id/lineView">

                        <View
                            android:id="@+id/view4"
                            android:layout_width="@dimen/dp_4"
                            android:layout_height="@dimen/dp_15"
                            android:layout_marginTop="@dimen/dp_15"
                            android:background="@drawable/shape_line_vertical"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_goneMarginTop="@dimen/dp_15" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_7"
                            android:gravity="center"
                            android:text="我的社交主页"
                            android:textColor="@color/text_color_444444"
                            android:textSize="@dimen/sp_15"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="@id/view4"
                            app:layout_constraintStart_toEndOf="@+id/view4"
                            app:layout_constraintTop_toTopOf="@id/view4" />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                    <View
                        android:id="@+id/lineView2"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_10"
                        android:background="@color/dl_background"
                        app:layout_constraintTop_toBottomOf="@id/home_Con" />


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:paddingBottom="@dimen/dp_10"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/lineView2">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/group_Con"
                            android:layout_width="@dimen/dp_106"
                            android:layout_height="@dimen/dp_106"
                            android:layout_marginStart="@dimen/dp_15"
                            android:layout_marginTop="@dimen/dp_15"
                            android:background="@mipmap/card_groupbg"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                android:id="@+id/groupName_Tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dp_10"
                                android:layout_marginTop="@dimen/dp_10"
                                android:text="我的社群"
                                android:textColor="@color/white"
                                android:textSize="14sp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/joinCount_Tv"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dp_10"
                                android:layout_marginTop="@dimen/dp_5"
                                android:maxLines="1"
                                android:text="加入16个社群"
                                android:textColor="@color/white"
                                android:textSize="10sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/groupName_Tv" />


                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/interest_Con"
                            android:layout_width="@dimen/dp_106"
                            android:layout_height="@dimen/dp_106"
                            android:layout_marginStart="@dimen/dp_15"
                            android:layout_marginTop="@dimen/dp_15"
                            android:background="@drawable/base_gradual"
                            app:layout_constraintStart_toEndOf="@id/group_Con"
                            app:layout_constraintTop_toTopOf="parent">


                            <TextView
                                android:id="@+id/interestName_Tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dp_10"
                                android:layout_marginTop="@dimen/dp_10"
                                android:text="我的兴趣圈"
                                android:textColor="@color/text_color_444444"
                                android:textSize="14sp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/interestJoinCount_Tv"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dp_10"
                                android:layout_marginTop="@dimen/dp_5"
                                android:text="加入16个兴趣圈"
                                android:textColor="@color/text_color_a1a1a1"
                                android:textSize="10sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/interestName_Tv" />

                            <LinearLayout
                                android:id="@+id/interestHead_Lin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dp_5"
                                android:layout_marginBottom="@dimen/dp_15"
                                android:orientation="horizontal"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent" />


                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.core.widget.NestedScrollView>
        </com.google.android.material.appbar.CollapsingToolbarLayout>

    </com.google.android.material.appbar.AppBarLayout>

    <LinearLayout
        android:id="@+id/bot_Lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <com.google.android.material.tabs.TabLayout
            android:id="@+id/card_Tab"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_50"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/title_bar"
            app:tabBackground="@color/transparent"
            app:tabIndicatorColor="@color/transparent"
            app:tabIndicatorHeight="@dimen/dp_0"
            app:tabMode="scrollable"
            app:tabTextAppearance="@style/TabLayoutTextStyle"
            app:tabTextColor="@color/text_color_6f6f6f" />

        <androidx.viewpager.widget.ViewPager
            android:id="@+id/card_ViewPager"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>

</com.qingbo.monk.base.CustomCoordinatorLayout>


