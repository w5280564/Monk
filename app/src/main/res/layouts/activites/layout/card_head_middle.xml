<?xml version="1.0" encoding="utf-8"?>
<com.qingbo.monk.base.baseview.DisInterceptNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/top_Src"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:tag="middle"
    app:layout_collapseParallaxMultiplier="0.8">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/top_Con"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_72"
        app:layout_constraintTop_toBottomOf="@id/back_Btn"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/editUser_Tv_"
            android:layout_width="@dimen/dp_67"
            android:layout_height="@dimen/dp_28"
            android:layout_marginEnd="@dimen/dp_15"
            android:background="@drawable/card_edit"
            android:drawableStart="@mipmap/bianji_pen"
            android:drawablePadding="@dimen/dp_10"
            android:drawableTint="@color/white"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/dp_10"
            android:text="编辑"
            android:textColor="@color/white"
            android:textSize="12sp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="@id/head_Img" />

        <TextView
            android:id="@+id/follow_Tv"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_22"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginEnd="@dimen/dp_20"
            android:background="@drawable/interest_lable"
            android:gravity="center"
            android:paddingStart="@dimen/dp_11"
            android:paddingEnd="@dimen/dp_11"
            android:text="关注"
            android:textColor="@color/text_color_444444"
            android:textSize="12sp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/send_Mes"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_22"
            android:layout_marginEnd="@dimen/dp_10"
            android:background="@drawable/message_bg"
            android:gravity="center"
            android:paddingStart="@dimen/dp_8"
            android:paddingEnd="@dimen/dp_8"
            android:text="私信"
            android:textColor="@color/text_color_6f6f6f"
            android:textSize="10sp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@id/follow_Tv"
            app:layout_constraintEnd_toStartOf="@id/follow_Tv"
            app:layout_constraintTop_toTopOf="@id/follow_Tv" />


        <ImageView
            android:id="@+id/head_Img"
            android:layout_width="@dimen/dp_56"
            android:layout_height="@dimen/dp_56"
            android:layout_marginStart="@dimen/dp_15"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_18"
            android:maxLength="6"
            android:text="一位神秘的人"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/head_Img"
            app:layout_constraintTop_toBottomOf="@+id/head_Img" />

        <ImageView
            android:id="@+id/sex_Img"
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_marginStart="@dimen/dp_16"
            android:baselineAlignBottom="true"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_name"
            app:layout_constraintBottom_toBottomOf="@+id/tv_name"
            app:layout_constraintStart_toEndOf="@+id/tv_name" />

        <TextView
            android:id="@+id/tv_follow_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_13"
            android:text="55"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_name" />

        <TextView
            android:id="@+id/textview10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:text="关注"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_10"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_follow_number"
            app:layout_constraintStart_toEndOf="@+id/tv_follow_number" />

        <View
            android:id="@+id/line"
            android:layout_width="@dimen/dp_1"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/dp_13"
            app:layout_constraintBottom_toBottomOf="@id/tv_follow_number"
            app:layout_constraintStart_toEndOf="@id/textview10"
            app:layout_constraintTop_toTopOf="@id/tv_follow_number" />


        <TextView
            android:id="@+id/tv_fans_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:text="55"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_15"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@id/tv_follow_number"
            app:layout_constraintStart_toEndOf="@id/line"
            app:layout_constraintTop_toTopOf="@id/tv_follow_number"
            app:layout_goneMarginStart="@dimen/sp_10" />

        <TextView
            android:id="@+id/textview11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_5"
            android:text="粉丝"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_10"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_fans_number"
            app:layout_constraintStart_toEndOf="@id/tv_fans_number" />


        <com.qingbo.monk.base.baseview.ExpandTextView
            android:id="@+id/brief_Tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_17"
            android:layout_marginEnd="@dimen/dp_25"
            android:maxLines="3"
            android:textColor="@color/white"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_follow_number" />


        <LinearLayout
            android:id="@+id/label_Lin"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_17"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/brief_Tv" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_19"
            android:background="@drawable/base_gradual_top"
            app:layout_constraintTop_toBottomOf="@id/label_Lin">

            <View
                android:id="@+id/view3"
                android:layout_width="@dimen/dp_4"
                android:layout_height="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@drawable/shape_line_vertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_goneMarginTop="@dimen/dp_20" />

            <TextView
                android:id="@+id/data_Tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_7"
                android:gravity="center"
                android:text="基本资料"
                android:textColor="@color/text_color_444444"
                android:textSize="@dimen/sp_15"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@id/view3"
                app:layout_constraintStart_toEndOf="@+id/view3"
                app:layout_constraintTop_toTopOf="@id/view3" />

            <TextView
                android:id="@+id/address_Tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_19"
                android:drawableStart="@mipmap/weizhi"
                android:drawablePadding="@dimen/dp_10"
                android:text="城市："
                android:textColor="@color/text_color_6f6f6f"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/data_Tv" />

            <TextView
                android:id="@+id/industry_Tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_15"
                android:drawableStart="@mipmap/hangye"
                android:drawablePadding="@dimen/dp_10"
                android:text="行业："
                android:textColor="@color/text_color_6f6f6f"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/address_Tv" />

            <TextView
                android:id="@+id/job_Tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_10"
                android:drawableStart="@mipmap/jingyan"
                android:drawablePadding="@dimen/dp_10"
                android:text="工作经验："
                android:textColor="@color/text_color_6f6f6f"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/industry_Tv" />

            <View
                android:id="@+id/lineView"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_10"
                android:background="@color/dl_background"
                app:layout_constraintTop_toBottomOf="@id/job_Tv" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/home_Con"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingBottom="@dimen/dp_30"
                app:layout_constraintTop_toBottomOf="@id/lineView">

                <View
                    android:id="@+id/view4"
                    android:layout_width="@dimen/dp_4"
                    android:layout_height="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_15"
                    android:background="@drawable/shape_line_vertical"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_goneMarginTop="@dimen/dp_15" />

                <TextView
                    android:id="@+id/mesHomepage_Tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_7"
                    android:gravity="center"
                    android:text="我的社交主页"
                    android:textColor="@color/text_color_444444"
                    android:textSize="@dimen/sp_15"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/view4"
                    app:layout_constraintStart_toEndOf="@+id/view4"
                    app:layout_constraintTop_toTopOf="@id/view4" />

                <TextView
                    android:id="@+id/urlLabelNone_Tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="暂未填写任何社交主页"
                    android:textColor="@color/text_color_6f6f6f"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@+id/mesHomepage_Tv"
                    app:layout_constraintTop_toBottomOf="@+id/mesHomepage_Tv" />

                <LinearLayout
                    android:id="@+id/urlLabel_Lin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintTop_toBottomOf="@id/mesHomepage_Tv" />

            </androidx.constraintlayout.widget.ConstraintLayout>


            <View
                android:id="@+id/lineView2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_10"
                android:background="@color/dl_background"
                app:layout_constraintTop_toBottomOf="@id/home_Con" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/group_Con"
                android:layout_width="@dimen/dp_106"
                android:layout_height="@dimen/dp_106"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_10"
                android:background="@mipmap/card_groupbg"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/lineView2">

                <TextView
                    android:id="@+id/groupName_Tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/joinCount_Tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_5"
                    android:maxLines="1"
                    android:textColor="@color/white"
                    android:textSize="10sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/groupName_Tv" />

                <LinearLayout
                    android:id="@+id/joinHead_Lin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/interest_Con"
                android:layout_width="@dimen/dp_106"
                android:layout_height="@dimen/dp_106"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@drawable/base_gradual"
                app:layout_constraintStart_toEndOf="@id/group_Con"
                app:layout_constraintTop_toBottomOf="@id/lineView2">

                <TextView
                    android:id="@+id/interestName_Tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textColor="@color/text_color_444444"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/interestJoinCount_Tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_5"
                    android:textColor="@color/text_color_6f6f6f"
                    android:textSize="10sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/interestName_Tv" />

                <LinearLayout
                    android:id="@+id/interestHead_Lin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</com.qingbo.monk.base.baseview.DisInterceptNestedScrollView>